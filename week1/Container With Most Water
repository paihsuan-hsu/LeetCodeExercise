    {
      "cell_type": "code",
      "source": [
        "\n",
        "int maxArea(vector<int>& height){\n",
        "    int right = height.szie()-1;\n",
        "    int left = 0;\n",
        "    int max_area = 0, area = 0;\n",
        "    while(left<right){\n",
        "        area = min(hight[left], hight[right]) * (right-left);\n",
        "        max_area = max(max_area, area);\n",
        "        if(hight[left]<hight[right]){\n",
        "            left++;\n",
        "        } else {\n",
        "            right--;\n",
        "        }\n",
        "    }\n",
        "    return max_area;\n",
        "}\n"
      ],
      "metadata": {
        "id": "zd4BKKh2MSN0"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "!pip install typing\n",
        "import typing\n",
        "def maxArea(self, height: typing.List[int]) -> int:\n",
        "        l = 0\n",
        "        r = len(height)-1\n",
        "        max_area = 0\n",
        "        area = 0\n",
        "\n",
        "        while l < r:\n",
        "            area = (r-l) * min(height[r], height[l])\n",
        "            max_area = max(max_area, area)\n",
        "            if height[l] < height[r]:\n",
        "                l+=1\n",
        "            else:\n",
        "                r-=1\n",
        "\n",
        "        return max_area"
      ],
      "metadata": {
        "id": "dOrguf08L-qv"
